            <string>/// save the deck file


window_set_cursor(cr_arrow) // change the cursor to normal (after loading)

file_name = global.custom_deck[global.custom_deck_number] +string(".deck") // set a suggested filename (custom deck name)


user_directory = get_save_filename("Deck File|*.deck",file_name) // OPEN DIALOG BOX

msg = instance_create(x,y,obj_message)
msg.type = ""

if user_directory = "" // if cancel was pressed
{
    msg.text = "No file was saved"
    msg.type = "WARNING"
} 
else 
{
    scr_export_custom_deck()
    msg.text = "File Saved!#"+string(user_directory)
    global.deck_directory = user_directory // save the directory of the custom deck
}


// if no deck name was made before saving, name deck untitled (avoids not loading empty deck)
if global.custom_deck[global.custom_deck_number] = ""
{global.custom_deck[global.custom_deck_number] = "Untitled"}


// reset user_directory to the temp directory to also save the deck there
user_directory = (working_directory + "custom_deck_" +string(global.custom_deck_number) +string(".deck"))
scr_save_custom_deck_file() // also save the deck to the working directory

window_set_fullscreen(global.set_fullscreen) // reset to fullscreen (if game was fullscreen)

scr_zoom_out()

scr_save_game() // updates game directories etc


</string>
